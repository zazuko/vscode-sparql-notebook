PREFIX gont: <https://gont.ch/>
PREFIX schema: <http://schema.org/>
SELECT ?name ?abbr ?date WHERE {
    { 
        ?canton a gont:Canton;
            gont:cantonAbbreviation ?abbr ;
            gont:date ?date ;
            gont:longName ?name . 
    } UNION {
        gont:Canton ?p ?o .
    }
} 
